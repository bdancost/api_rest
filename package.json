{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check-env": "ts-node scripts/checkEnv.ts",
    "dev": "npm run check-env && nodemon --exec ts-node src/app.ts",
    "start": "npm run check-env && node dist/app.js",
    "build": "tsc",
    "migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "ts-node node_modules/.bin/sequelize db:migrate:undo",
    "seed": "ts-node node_modules/.bin/sequelize db:seed:all",
    "test": "NODE_ENV=test jest --config jest.config.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.15",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
